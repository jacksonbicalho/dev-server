{
  "name": "ssl-dev",
  "version": "0.0.7-dev",
  "description": "Servidor https de desenvolvimento",
  "author": "Jackson Bicalho",
  "license": "MIT",
  "homepage": "https://github.com/jacksonbicalho/ssl-dev",
  "files": [
    "dist",
    "bin",
    "lib",
    "cli",
    "server",
    "utils",
    "config"
  ],
  "main": "./dist/index.js",
  "bin": {
    "ssl": "./dist/bin/ssl.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && cp -r src/lib/cli/mkcert.sh ./dist/lib/cli/mkcert.sh",
    "prepublishOnly": "yarn build && yarn pack",
    "mkcert": "node ./dist/lib/cli/mkcert.js",
    "setup": "node ./dist/lib/cli/setup.js",
    "start": "node ./dist/main.js",
    "dev": "nodemon",
    "test": "jest",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^11.1.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "global": "^4.4.0",
    "import-local": "^3.1.0",
    "node-gyp": "^10.0.1",
    "prettier": "^3.1.0",
    "serve-handler": "^6.1.5",
    "user": "^0.0.0"
  },
  "devDependencies": {
    "@types/exit": "^0.1.33",
    "@types/import-local": "^3.1.2",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/prompts": "^2.4.8",
    "@types/serve-handler": "^6.1.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "types-package-json": "^2.0.39",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
